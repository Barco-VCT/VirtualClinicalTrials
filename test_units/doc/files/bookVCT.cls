  \addtocontents{lof}{\protect\addvspace{10\p@}}
  \addtocontents{lot}{\protect\addvspace{10\p@}}
  \if@twocolumn
    \if@mainmatter                         
      \@topnewpage[\@makechapterhead{#2}]
    \else                                  
      \@topnewpage[\@makeschapterhead{#2}]
    \fi                                    
  \else
    \if@mainmatter                         
      \@makechapterhead{#2}%
    \else                                 
      \@makeschapterhead{#2}%              
    \fi                                    
    \@afterheading
  \fi}
